#include <iostream>

 

int main()

{

    float a,b,c;

    std::cout<<"enter the three side of thr triangle :";

    std::cin>>a>>b>>c;

    if(a==b && b==c){

        std::cout<<"the triangle is an equlateral triangle";

    }

    else if((a!=b && b==c) || (a==b && b!=c) || (a==c && b!=c)){

        std::cout<<"the triangle is an isosolesc triangle";

    }

    else

    {

        std::cout<<"the triangle is a scalen "; 

    }

    

} 
